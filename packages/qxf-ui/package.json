{
  "name": "qxf-ui-study",
  "type": "module",
  "version": "1.0.1",
  "description": "this is a vue3 component library",
  "author": "nanonanami <ns244444@163.com>",
  "license": "MIT",
  "keywords": [
    "vue3",
    "vue",
    "components"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./es/*": "./es/*",
    "./dist/*": "./dist/*",
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "main": "es/index.js",
  "types": "es/index.d.ts",
  "publishConfig": {
    "access": "public"
  },

  "files": [
    "dist", 
    "es", 
    "lib"
  ],
  "scripts": {
    "build": "vite build",
    "build:umd": "vite build --config vite.umd.config.ts",
    "build:style": "vite build --config vite.style.config.ts"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.4",
    "@qxf-ui-study/utils": "workspace:*",
    "lodash-es": "^4.17.21",
    "@v-c/utils": "^0.0.26"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12"
  }
}
